{
	"name": "rfp-frontend",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite --host --mode=dev",
		"prevbuild": "tsc && npm run lint && vite build",
		"devbuild": "npm run prevbuild -- --mode=dev",
		"qabuild": "npm run prevbuild -- --mode=qa",
		"stagingbuild": "npm run prevbuild -- --mode=staging",
		"prodbuild": "npm run prevbuild -- --mode=production",
		"lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
		"lint:format": "prettier  --log-level warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
		"lint": "npm run lint:format && npm run lint:fix ",
		"lint:staged": "pretty-quick --staged && lint-staged",
		"preview": "vite preview",
		"prepare": "husky install"
	},
	"dependencies": {
		"@ant-design/icons": "^5.2.6",
		"@handsontable/react": "^14.6.1",
		"@heropana/chat-ui-kit-react": "^2.0.6",
		"@reduxjs/toolkit": "^2.1.0",
		"antd": "^5.13.2",
		"axios": "^1.6.6",
		"formik": "^2.4.5",
		"fuse.js": "^7.0.0",
		"handsontable": "^14.6.1",
		"js-cookie": "^3.0.5",
		"martinez-polygon-clipping": "^0.7.3",
		"pretty-quick": "^4.0.0",
		"quill": "^2.0.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-promise-tracker": "^2.1.1",
		"react-quill": "^2.0.0",
		"react-redux": "^9.1.0",
		"react-router-dom": "^6.21.3",
		"react-toastify": "^10.0.4",
		"react-virtualized-auto-sizer": "^1.0.24",
		"react-window": "^1.8.10",
		"redux-persist": "^6.0.0",
		"sass": "^1.70.0",
		"socket.io-client": "^4.7.5",
		"spinners-react": "^1.0.7",
		"xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
		"yup": "^1.3.3"
	},
	"devDependencies": {
		"@types/js-cookie": "^3.0.6",
		"@types/react": "^18.2.43",
		"@types/react-dom": "^18.2.17",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.14.0",
		"@vitejs/plugin-react": "^4.2.1",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"autoprefixer": "^10.4.17",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-config-standard-with-typescript": "^43.0.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-n": "^16.6.2",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"eslint-plugin-sonarjs": "^1.0.4",
		"eslint-plugin-unused-imports": "^4.1.3",
		"husky": "^9.0.7",
		"lint-staged": "^15.2.0",
		"prettier": "3.2.4",
		"tailwindcss": "^3.4.1",
		"typescript": "^5.3.3",
		"vite": "^5.0.8"
	},
	"hooks": {
		"pre-commit": "npm run lint:staged"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json,css,js}": [
			"npm run lint",
			"git add ."
		]
	}
}
